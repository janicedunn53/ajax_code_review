<%= render 'layouts/errors', :object => @listing %>

<%= form_for [@user, @listing], html: { multipart: true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :photo %>
  <%= f.file_field :photo %>
  <br>
  <%= f.submit  %>
<% end %>
